ys[i]; if (excluded.indexOf(key) >= 0) continue; target[key] = source[key]; } return target; }\n\n/* @jsxRuntime classic */\n\n/* @jsx mdx */\nvar _frontmatter = {\n  \"title\": \"Alt.Ctrl.GDC 2018 Award\",\n  \"type\": \"project\",\n  \"published\": true,\n  \"date\": \"2018-03-20T00:00:00.000Z\",\n  \"skills\": [\"software dev\", \"physical experience\"],\n  \"tags\": [\"alt ctrl\", \"commissioned\"],\n  \"summary\": \"An IGF commissioned, highly interactive award.\",\n  \"presskit\": null,\n  \"external_link\": null,\n  \"thumbnail\": \"alt-ctrl-award.jpg\",\n  \"slides\": [\"alt-ctrl-award-laser-plan.jpg\", \"alt-ctrl-award-laser-cutting-01.jpg\", \"alt-ctrl-award-board.jpg\", \"alt-ctrl-award-electronics.jpg\", \"alt-ctrl-award-mockup-02.jpg\", \"alt-ctrl-award-install-02.jpg\", \"alt-ctrl-award-install-03.jpg\"],\n  \"images\": [\"alt-ctrl-award.jpg\"],\n  \"gallery\": [\"alt-ctrl-award.jpg\", \"alt-ctrl-award-install-03.jpg\", \"alt-ctrl-award-install-02.jpg\", \"alt-ctrl-award-mockup-02.jpg\", \"alt-ctrl-award-electronics.jpg\", \"alt-ctrl-award-board.jpg\", \"alt-ctrl-award-laser-cutting-01.jpg\", \"alt-ctrl-award-laser-plan.jpg\"],\n  \"credits\": [{\n    \"name\": \"Jerry Belich\",\n    \"link\": \"https://twitter.com/j3rrytron\",\n    \"role\": \"Creator\"\n  }],\n  \"recognition\": [{\n    \"event\": \"IGF Alt.Ctrl.GDC\",\n    \"status\": \"It was the Alt.Ctrl.GDC Award!\",\n    \"location\": \"San Francisco, CA\",\n    \"dates\": \"03-20-2018\",\n    \"datesPretty\": \"March 20, 2018\"\n  }]\n};\nvar layoutProps = {\n  _frontmatter: _frontmatter\n};\nvar MDXLayout = \"wrapper\";\nreturn function MDXContent(_ref) {\n  var components = _ref.components,\n      props = _objectWithoutProperties(_ref, _excluded);\n\n  return mdx(MDXLayout, _extends({}, layoutProps, props, {\n    components: components,\n    mdxType: \"MDXLayout\"\n  }), mdx(\"p\", null, \"This was a commissioned, highly interactive award for the winner of the IGF Alt.Ctrl.GDC category in 2018. It is a mock television with eight virtual channels, a power switch with brightness setting, a warm flickering LED backlight, conductive antennas, and a thermal printer for the interface and screen. The complex enclosure was designed using \", mdx(\"a\", {\n    parentName: \"p\",\n    \"href\": \"https://www.tinkercad.com/\"\n  }, \"Tinkercad\"), \", online modeling software by Autodeck. It was used to design and test the numerous laser cut panels required to build the final 3D form and support all of the electronics required. The most challenging aspect was perfecting the placement of the thermal printer so that it could push the paper through the panels into the viewport and not get jammed as it continued out the top.\"), mdx(\"blockquote\", null, mdx(\"iframe\", {\n    parentName: \"blockquote\",\n    \"width\": \"100%\",\n    \"height\": 315,\n    \"src\": \"https://www.youtube-nocookie.com/embed/QY2zGI-o5ik?rel=0\",\n    \"frameBorder\": \"0\",\n    \"allow\": \"accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture\",\n    \"allowFullScreen\": true\n  })), mdx(\"p\", null, \"When the TV is first turned on the screen prints nothing but static, random characters that fill the viewing space. Changing the channels simply prints different screens of more static. If you adjust the antennas, after a few seconds the static will clear and the current channel will print out a scene of ascii art representing the content of that channel. It could be a scary movie, a nature show, cooking show, sci-fi, etc. But one of the channels is the IGF Awards show. It will print an initial screen saying as much as well as the impending winner of the award. After a few seconds the \\u2018screen\\u2019 will update printing out the actual winner of the award itself, completing the journey.\"), mdx(\"h2\", null, \"Electronics Used\"), mdx(\"ul\", null, mdx(\"li\", {\n    parentName: \"ul\"\n  }, mdx(\"a\", {\n    parentName: \"li\",\n    \"href\": \"https://www.adafrui